","original":{"line":135,"column":5}},{"generated":{"line":153,"column":0},"source":"createPage.js","original":{"line":5,"column":43}},{"generated":{"line":153,"column":2},"source":"createPage.js","original":{"line":139,"column":2}},{"generated":{"line":154,"column":0},"source":"createPage.js","original":{"line":140,"column":2}},{"name":"customElements","generated":{"line":158,"column":0},"source":"createPage.js","original":{"line":142,"column":2}},{"generated":{"line":158,"column":14},"source":"createPage.js","original":{"line":142,"column":16}},{"name":"define","generated":{"line":158,"column":15},"source":"createPage.js","original":{"line":142,"column":17}},{"generated":{"line":158,"column":21},"source":"createPage.js","original":{"line":142,"column":2}},{"generated":{"line":158,"column":22},"source":"createPage.js","original":{"line":142,"column":24}},{"generated":{"line":158,"column":35},"source":"createPage.js","original":{"line":142,"column":2}},{"name":"createPage","generated":{"line":158,"column":37},"source":"createPage.js","original":{"line":142,"column":38}},{"generated":{"line":158,"column":47},"source":"createPage.js","original":{"line":142,"column":2}}],"sources":{"createPage.js":"\r\nimport { css, LitElement, html } from './lit-element/lit-element.js';\r\nimport { Link, Routal } from '/routal.js'\r\n\r\nexport class createPage extends LitElement {\r\n\r\n    static get properties(){\r\n        return {\r\n            title:String,\r\n            city:String,\r\n            description:String,\r\n            type:String,\r\n            instagramHandle:String,\r\n        }\r\n    }\r\n\r\n    constructor() {\r\n        super()\r\n        this.type = 'person'\r\n    }\r\n\r\n    setValue(event){\r\n        let prop = event.target.getAttribute('id')\r\n        this[prop] = event.target.value\r\n    }\r\n\r\n    async requestNewPage(){\r\n        let putQuery = {fullName:this.fullName,type:this.type, city:this.city, description:this.description, instagramHandle:this.instagramHandle }\r\n        let newPage = await U.query({$write:{ on:'pages', put:putQuery }})\r\n        if(newPage.error){\r\n            return U.say(newPage.error)\r\n        }\r\n        Link('/page/'+newPage.title)\r\n    }\r\n\r\n    static get styles(){\r\n        return css`\r\n\r\n            center{\r\n                font-family:roboto;\r\n            }\r\n\r\n            h3{\r\n                font-weight:100;\r\n            }\r\n\r\n            input,button{\r\n                display: inherit;\r\n                width: 62%;\r\n                border: 1px solid #000;\r\n                padding: 14px 21px;\r\n                font-size: 20px;\r\n                border-radius: 300px;\r\n                margin: 12px 0;\r\n                outline:none;\r\n                box-sizing: border-box;\r\n            }\r\n\r\n            button{\r\n                border: none;\r\n                background: #fe5542;\r\n                color: #fff;\r\n                padding: 12px 24px;\r\n                display: inline-block;\r\n                cursor:pointer;\r\n            }\r\n\r\n            .type button{\r\n                width:100%;\r\n                background: #99999921;\r\n                color:#222;\r\n            }\r\n\r\n            .type{\r\n                display:grid;\r\n                width:62%;\r\n                grid-template-columns:50% 50%;\r\n            }\r\n\r\n            .type button[data-active=true]{\r\n                background:tomato;\r\n                color:#fff;\r\n            }\r\n\r\n            .type > :first-child{\r\n                border-radius:50px 0 0 50px;\r\n            }\r\n\r\n            .type > :nth-child(2){\r\n                border-radius:0 50px 50px 0;\r\n            }\r\n\r\n            @media (min-width:850px){\r\n                input,button,.type{\r\n                    width:38vw;\r\n                }\r\n\r\n            }\r\n        `\r\n    }\r\n\r\n    isActive(type){\r\n  \r\n        return this.type == type? true: false\r\n    }\r\n\r\n    setActive(e){\r\n        this.type = e.target.dataset.type\r\n    }\r\n  \r\n    render(){\r\n    \r\n\r\n      return html`\r\n      <center>\r\n            <h3> Create Roasting Page </h3>\r\n            <div class=\"type\">\r\n                <button data-type=\"person\" @click=\"${this.setActive.bind(this)}\" data-active=\"${this.isActive('person')}\">Person</button> \r\n                <button data-type=\"topic\" @click=\"${this.setActive.bind(this)}\" data-active=\"${this.isActive('topic')}\">Topic</button>\r\n            </div>\r\n            \r\n            <input @keyup=\"${this.setValue.bind(this)}\" id=\"fullName\" placeholder=\"Name\">\r\n            <input @keyup=\"${this.setValue.bind(this)}\" id=\"description\" placeholder=\"short description\">\r\n\r\n            ${this.type == 'person'?html`\r\n                <input @keyup=\"${this.setValue.bind(this)}\" id=\"instagramHandle\" placeholder=\"instagram username\">\r\n                <input @keyup=\"${this.setValue.bind(this)}\" id=\"city\" placeholder=\"city\">\r\n            `:html``}\r\n\r\n\r\n            <button @click=\"${this.requestNewPage.bind(this)}\"> create </button>\r\n            \r\n        </center>\r\n      `\r\n    }\r\n  \r\n  }\r\n\r\n  //instagram id is an essential field as users can easily find new photos that\r\n  //could be used as roasts \r\n\r\n  customElements.define('create-page',createPage)"},"lineCount":null}},"error":null,"hash":"641d1c7231a7a86b2cc41954d51d6322","cacheData":{"env":{}}}