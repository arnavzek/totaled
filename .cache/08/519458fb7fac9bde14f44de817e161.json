tValue = e.target.value\r\n    }\r\n\r\n    static get styles(){\r\n      return css`\r\n\r\n        #container{\r\n          display:grid;\r\n          place-items:center;\r\n          font-family:roboto;\r\n        }\r\n\r\n        input,button{\r\n          width: 62%;\r\n          border: 1px solid #000;\r\n          padding: 14px 21px;\r\n          font-size: 20px;\r\n          border-radius: 300px;\r\n          margin: 12px 0;\r\n          outline:none\r\n      }\r\n\r\n      button{\r\n          border: none;\r\n          background: #fe5542;\r\n          color: #fff;\r\n          padding: 12px 24px;\r\n          display: inline-block;\r\n          width: 67%;\r\n      }\r\n\r\n      `\r\n    }\r\n  \r\n    render(){\r\n\r\n      return html`\r\n      <div id=\"container\">\r\n        <h3>Roast by Sarcastic Bio</h3>\r\n\r\n\r\n        <input data-type=\"field\" value=${this.textField} @keyup=\"${this.updateValue.bind(this)}\" placeholder=\"field\">\r\n        <input data-type=\"value\" value=${this.textValue} @keyup=\"${this.updateValue.bind(this)}\" placeholder=\"value\">\r\n\r\n        <button @click=\"${this.publish.bind(this)}\"> Add </button>\r\n\r\n      </div>\r\n      `\r\n    }\r\n  \r\n}\r\n\r\ncustomElements.define('add-bio',addBio)"},"lineCount":null}},"error":null,"hash":"a6b9d78565736dd7006bf8391ee60a29","cacheData":{"env":{}}}