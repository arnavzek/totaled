port { Link, Routal } from '/routal.js'\r\n\r\n\r\n\r\nexport class header extends LitElement {\r\n\r\n    static get properties() {\r\n\r\n      return {\r\n        introMessageToRender: { type: Object },\r\n        othersField: { type: String },//sets the button in the top right corner\r\n        loggedOutAttribute:{type:String},//used to set height of header\r\n        searchQuery:{type:String}\r\n      }\r\n\r\n    }\r\n  \r\n  \r\n    constructor() {\r\n\r\n      super()\r\n      this.searchQuery = ''\r\n      this.loggedOutAttribute = ''\r\n      this.introComponent = html`\r\n              \r\n              <h1>A roasting platform</h1>\r\n              <a @click='${U.forceLogin}' >Sign up to roast Friends & Family</a>   \r\n              `\r\n\r\n    }\r\n  \r\n  \r\n    firstUpdated(){\r\n  \r\n      //intro will only be shown when user is not logged in and is on the home page\r\n\r\n        let morePannel = `\r\n\r\n          <button click=\"logout\" > Logout </button>\r\n\r\n        `\r\n\r\n          let gotUserName = (userName)=>{\r\n            \r\n            if(userName){\r\n              this.othersField = html`<button @click='${()=>{U.say(morePannel)}}' >\r\n              \r\n                <svg viewBox=\"0 0 15 15\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\" width=\"15\" height=\"15\">\r\n                  <path d=\"M15 3.5H0m15 8H6m9-4H9\" stroke=\"currentColor\"></path>\r\n                </svg>\r\n              \r\n              </button>`\r\n              this.introMessageToRender = ''\r\n            }else{\r\n              this.loggedOutAttribute = 'loggedOutAttribute'\r\n              this.othersField = html`<button @click='${U.forceLogin}'>login</button>`\r\n  \r\n              let config = [\r\n                {pattern:'*',transitionType:'flip',transitionAxis:'x',component:()=>{ return this.introComponent}},\r\n              ]\r\n  \r\n  \r\n  \r\n              setTimeout(()=>{\r\n                Routal.setup(config,(component)=>{\r\n                \r\n                this.introMessageToRender = component\r\n              },this.shadowRoot.querySelector('#container'))\r\n              },100)\r\n  \r\n  \r\n            }\r\n          }\r\n  \r\n          U.readUser('username').then(gotUserName)\r\n    }\r\n  \r\n    static get styles(){\r\n  \r\n      return css`\r\n  \r\n      \r\n\r\n\r\n        #header{\r\n            background: linear-gradient(180.74deg, #F8BF50 0.93%, #FF3F3F 83.05%);\r\n            height:auto;\r\n            min-height: 22vh;\r\n            width:100vw;\r\n            position:relative;\r\n            font-family:Rochester, sans-serif;\r\n            transition:all 1s ease;\r\n        }\r\n  \r\n        #header.loggedOutAttribute{\r\n          min-height:95vh;\r\n        }\r\n\r\n        #logo a{\r\n          display:none;\r\n        }\r\n\r\n        #logo{\r\n          cursor:pointer;\r\n        }\r\n  \r\n        #head{\r\n          grid-template-columns: 19% 62% 19%;\r\n          display: grid;\r\n          padding:24px;\r\n          padding-top:12px;\r\n        }\r\n  \r\n  \r\n        #head #logo img{\r\n          width: 45px;\r\n          margin-top: 3px;\r\n        }\r\n  \r\n        #head #logo span{\r\n          font-size:15px;\r\n          color:#fff;\r\n          padding-bottom:6px;\r\n          font-weight:900;\r\n          line-height:21px;\r\n          padding-top:3px;\r\n        }\r\n  \r\n        #intro{\r\n          display: inline-block;\r\n          width: 100%;\r\n          margin-bottom: 0;\r\n        }\r\n  \r\n        #head #search{\r\n          width:83%;\r\n          border:none;\r\n          border-radius:300px;\r\n          text-align: center;\r\n          outline:none;\r\n          background:#ffffff78;\r\n        \r\n          margin-top: 0;\r\n          width: 38vw;\r\n          height: 39px;\r\n          justify-self:center;\r\n          padding: 0;\r\n          height: 45px;\r\n        }\r\n  \r\n\r\n  \r\n        #flame{\r\n          position:absolute;\r\n          left: 0;\r\n          width:100%;\r\n          background-image: url(/images/header/flame.svg);\r\n          background-repeat: repeat-x;\r\n          width: 100vw;\r\n          bottom: 0;\r\n\r\n          height: 120px;\r\n          background-size: cover;\r\n          margin-bottom: -6px;\r\n          background-position: center;\r\n        }\r\n  \r\n        #intro h1{\r\n          margin-top:18px;\r\n          margin-bottom:6px;\r\n          font-size:35px;\r\n          color:#fff;\r\n        }\r\n  \r\n        #intro a{\r\n          padding:9vw 0vw;\r\n          border:2px solid;\r\n          width:38vw;\r\n          font-size:6px;\r\n          display: inline-block;\r\n          text-decoration:none;\r\n          border-radius:300px;\r\n          font-family:sans-serif;\r\n          color:#fff;\r\n        }\r\n  \r\n        button{\r\n          cursor:pointer;\r\n        }\r\n\r\n        #others{\r\n\r\n          justify-self:right;\r\n\r\n        }\r\n  \r\n        #others button{\r\n          background:transparent;\r\n          border:none;\r\n          justify-self: right;\r\n          padding: 0;\r\n          margin: 0;\r\n          height: 40px;\r\n        }\r\n\r\n        #others button svg{\r\n\r\n\r\n          color: #fff;\r\n\r\n          height: 40px;\r\n          width: 40px;\r\n          outline:none;\r\n          border: none;\r\n        }\r\n  \r\n        a:hover,button:hover{\r\n          filter:inver(1);\r\n          transition:all 0.5s ease;\r\n        }\r\n\r\n        @media (min-width: 850px) {\r\n          #flame {\r\n            background-size: contain;\r\n            height: 60px;\r\n          }\r\n        }\r\n  \r\n      `\r\n    }\r\n\r\n    search(event){\r\n      this.searchQuery = event.target.value\r\n      if(event.keyCode !== 13) return\r\n      Link('/search/'+event.target.value)\r\n    }\r\n  \r\n    render(){\r\n  \r\n      return html`\r\n  \r\n      <div id='header' class='${this.loggedOutAttribute}'>\r\n        <div id='head'>\r\n\r\n          <div @click=\"${()=>(Link('/'))}\" id='logo' >\r\n              <img src='/images/header/logo.svg'>\r\n              <a  href=\"/\">Totaled</a>\r\n          </div>\r\n\r\n         \r\n          <input @keyup=\"${this.search.bind(this)}\" id='search' value='${this.searchQuery}' placeholder='Search Totaled'>\r\n\r\n          <div id='others'>${this.othersField}</div>\r\n         \r\n\r\n\r\n\r\n\r\n        </div>\r\n  \r\n        <div id='container'>\r\n          <center id='intro'>\r\n          ${this.introMessageToRender} \r\n          </center>\r\n        </div>\r\n  \r\n        <div id='flame'></div>\r\n      </div>\r\n  \r\n      `\r\n    }\r\n  }\r\n   \r\n  customElements.define('the-header', header);"},"lineCount":null}},"error":null,"hash":"321bbe8186ec72fe6927688e3a0fd901","cacheData":{"env":{}}}